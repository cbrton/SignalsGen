    LCU4_FN_KE1_1_OPEN  : BOOL; //  Арматура с электроприводом 3zpe1_1 - открыть    BOOL    0-BIT 0     "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    LCU4_FN_KE1_2_OPEN  : BOOL; //  Арматура с электроприводом 3zpe1_2 - открыть    BOOL    0-BIT 1     "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    LCU4_FN_KE1_3_OPEN  : BOOL; //  Арматура с электроприводом 3zpe1_3 - открыть    BOOL    0-BIT 2     "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    LCU4_FN_KE2_1_OPEN  : BOOL; //  Арматура с электроприводом 3zpe2_1 - открыть    BOOL    0-BIT 3     "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    LCU4_FN_KE2_2_OPEN  : BOOL; //  Арматура с электроприводом 3zpe2_2 - открыть    BOOL    0-BIT 4     "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    LCU4_FN_KE2_3_OPEN  : BOOL; //  Арматура с электроприводом 3zpe2_3 - открыть    BOOL    0-BIT 5     "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    LCU4_FN_KE3_1_OPEN  : BOOL; //  Арматура с электроприводом 3zpe3_1 - открыть    BOOL    0-BIT 6     "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    LCU4_FN_KE3_2_OPEN  : BOOL; //  Арматура с электроприводом 3zpe3_2 - открыть    BOOL    0-BIT 7     "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    LCU4_FN_KE3_3_OPEN  : BOOL; //  Арматура с электроприводом 3zpe3_3 - открыть    BOOL    0-BIT 8     "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    LCU4_FN_KE4_2_OPEN  : BOOL; //  Арматура с электроприводом 3zpe4_2 - открыть    BOOL    0-BIT 9     "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
