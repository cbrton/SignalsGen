    BP7_N4_4_IND                        : WORD; // Состояние - Насос Н4_4   WORD    160 0 - откл, 1 - вкл, 2 - авария   R   input register  0x04
    BP8_1_N5_1_IND                      : WORD; // Состояние - Насос Н5_1   WORD    161 0 - откл, 1 - вкл, 2 - авария, 3 - ожидание разгона R   input register  0x04
    BP8_1_N5_2_IND                      : WORD; // Состояние - Насос Н5_2   WORD    162 0 - откл, 1 - вкл, 2 - авария, 3 - ожидание разгона R   input register  0x04
