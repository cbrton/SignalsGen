    //  ЦШУ3_WORD_CMND2 - Слово команд №2
    REM_4KE6_OPEN           : BOOL; //  Арматура с электроприводом 4кшэ6 - открыть  BOOL    2-BIT 0 "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    REM_BP7_N4_1_ON_OFF     : BOOL; //  Насос Н4_1 ВКЛ_ОТКЛ BOOL    2-BIT 1 "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    REM_BP7_N4_2_ON_OFF     : BOOL; //  Насос Н4_2 ВКЛ_ОТКЛ BOOL    2-BIT 2 "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    REM_BP7_N4_4_ON_OFF     : BOOL; //  Насос Н4_4 ВКЛ_ОТКЛ BOOL    2-BIT 3 "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    REM_BP8_1_N5_1_ON_OFF   : BOOL; //  Насос Н5_1 ВКЛ_ОТКЛ BOOL    2-BIT 4 "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    REM_BP8_1_N5_2_ON_OFF   : BOOL; //  Насос Н5_2 ВКЛ_ОТКЛ BOOL    2-BIT 5 "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    REM_BP8_2_N5_3_ON_OFF   : BOOL; //  Насос Н5_3 ВКЛ_ОТКЛ BOOL    2-BIT 6 "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    REM_BP8_2_N5_4_ON_OFF   : BOOL; //  Насос Н5_4 ВКЛ_ОТКЛ BOOL    2-BIT 7 "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    
    //  ЦШУ3_WORD_CMND1 - Слово команд №1   WORD
    Rem_Reset_ALM   : BOOL; //  Внешний сигнал квитирования аварий  BOOL    3-BIT 0 "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    Rem_Stop        : BOOL; //  Внешний сигнал останова (аналогично Авар. ост.) BOOL    3-BIT 1 "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    Set_rem_mode    : BOOL; //  Внешний сигнал. Установить дистанционное подключение    BOOL    3-BIT 2 "0 - нет сигнала, 1 - есть сигнал"  RW  Holding register    0x06
    



    // Вспомогательные переменные
    arwMB_in    : ARRAY[0..200] OF WORD;  // Входные регистры (Input registers)
    arwMB_out   : ARRAY[0..10]  OF WORD;  // Регистры хранения (Holding registers)  
    quality     : INT;                    // Качество связи
